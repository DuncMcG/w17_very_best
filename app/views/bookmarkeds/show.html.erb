<div class="page-header">
  <h3>Bookmarked #<%= @bookmarked.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Entree</dt>
      <dd>
        <% if @bookmarked.entree.present? %>
          <a href="/entrees/<%= @bookmarked.entree_id %>">
            <%= @bookmarked.entree.dish_id %>
          </a>
        <% end %>
      </dd>

      <dt>User</dt>
      <dd>
        <% if @bookmarked.user.present? %>
          <a href="/users/<%= @bookmarked.user_id %>">
            <%= @bookmarked.user.email %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmarkeds" class="btn btn-primary">
        Back
      </a>
      <% if current_user == @bookmarked.user %>
        <a href="/bookmarkeds/<%= @bookmarked.id %>/edit" class="btn btn-warning">
          Edit
        </a>
        <a href="/delete_bookmarked/<%= @bookmarked.id %>" class="btn btn-danger" rel="nofollow">
          Delete
        </a>
      <% end %>
    </div>
  </div>
</div>
